.parallax,.parallax1, .parallax2, .parallax3 {
    background-image: url(http://www.andrealvarez.com/kobra/fondo_header.jpg);
    height: 400px;
    background-attachment: fixed;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.parallax1{
  background-image: url(http://www.andrealvarez.com/parkwash/images/slider1.jpg);
}

.parallax2{
  background-image: url(http://www.andrealvarez.com/parkwash/images/plantas.jpg);
}


.titular{
  padding-top:140px;
}

.franja{
  background-color: white;
  padding: 40px 0;
  color:#032C4C;
}

.franja h1{
  font-size: 2.5em;
  text-align: center;
}

.uno{
  background-image: url(http://www.andrealvarez.com/parkwash/images/fondo1.jpg);
  color:white;
  padding: 200px 0;
}

.textoCuerpo{
font-size: 1.5em;
}

.dos{
  color:black;
}

.dosIzq{
  padding: 150px 0;
}
.dosDer{
  background-image: url(http://www.andrealvarez.com/parkwash/images/fondo2.jpg);
}

.tres{
