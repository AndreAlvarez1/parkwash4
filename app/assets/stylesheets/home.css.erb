}


@media (min-width: 768px) {
  .parallax1{
    height: 600px;
  }
  .titular{
    padding-top:200px;
  }

  .para h1{
    color:white;
    font-size: 4em;
    text-transform: uppercase;
    text-align: center;
  }

  .para h2{
    color:white;
    font-size: 2em;
    text-transform: uppercase;
    text-align: center;
  }

}
